            <joint type="slide" axis="1 0 0"/>
            <!-- <joint type="slide" axis="0 1 0"/> -->
            <!-- <joint type="slide" axis="0 0 1"/> -->
            <joint name="r_hinge" type="hinge" axis="1 0 0" frictionloss="1000"/>
            <joint name="p_hinge" type="hinge" axis="0 1 0" frictionloss="0"/>
            <joint name="y_hinge" type="hinge" axis="0 0 1" frictionloss="1000"/>
